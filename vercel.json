{
  "version": 2,
  "builds": [
    {
      "src": "./back/server.js",  // path to your server.js file
      "use": "@vercel/node"
    },
    {
      "src": "./front/build",    // path to your build folder
      "use": "@vercel/static"
    }
  ],

  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/back/server.js"
    }
  ]
}